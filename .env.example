# Environment Configuration
# Copy this file to .env and fill in your values

# Development server URL (your local FastAPI backend)
DEV_SERVER_URL=http://localhost:8000

# Production server URL (your deployed FastAPI backend)
PROD_SERVER_URL=https://api.yourdomain.com

# Current environment (development or production)
NODE_ENV=development

# Optional: Public key for restaurant authentication
PUBLIC_KEY=your_public_key_here
