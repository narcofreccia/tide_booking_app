{
  "cli": {
    "version": ">= 5.2.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "APP_ENV": "development",
        "DEV_SERVER_URL": "http://localhost:8000",
        "PROD_SERVER_URL": "https://westpiersrl-a6a9f5c00dd1.herokuapp.com/"
      },
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "APP_ENV": "production",
        "DEV_SERVER_URL": "http://localhost:8000",
        "PROD_SERVER_URL": "https://westpiersrl-a6a9f5c00dd1.herokuapp.com/"
      },
      "ios": {
        "simulator": false
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "APP_ENV": "production",
        "DEV_SERVER_URL": "http://localhost:8000",
        "PROD_SERVER_URL": "https://westpiersrl-a6a9f5c00dd1.herokuapp.com/"
      },
      "android": {
        "gradleCommand": ":app:bundleRelease"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "andrea.peccerillo@gmail.com",
        "ascAppId": "6753350892",
        "appleTeamId": "D3HNV89F77"
      }
    }
  }
}
